{
    "compilerOptions": {
      "module": "commonjs",
      "target": "esnext",
      "lib": ["es2015", "webworker"],
      "jsx": "react",
      "noImplicitAny": true,
      "preserveConstEnums": true,
      "outDir": "./dist",
      "moduleResolution": "node",
      "baseUrl": "./",
      "paths": {
        "@*": ["src/*"],
    }
    },
    "include": ["src/*.ts", "src/**/*.ts", "src/*.tsx", "src/**/*.tsx"]
  }